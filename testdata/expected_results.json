{
  "description": "Expected Elo calculation results for test data validation",
  "initial_rating": 1200,
  "k_factor": 32,
  "test_cases": [
    {
      "description": "Basic rating calculation between two unrated proposals",
      "proposal_a": {
        "id": "PROP001",
        "title": "Building Scalable Microservices",
        "initial_rating": 1200
      },
      "proposal_b": {
        "id": "PROP002", 
        "title": "Advanced React Patterns",
        "initial_rating": 1200
      },
      "winner": "PROP001",
      "expected_results": {
        "PROP001": {
          "new_rating": 1216,
          "rating_change": 16
        },
        "PROP002": {
          "new_rating": 1184,
          "rating_change": -16
        }
      }
    },
    {
      "description": "Higher rated proposal defeats lower rated proposal",
      "proposal_a": {
        "id": "PROP007",
        "title": "Machine Learning in Production",
        "initial_rating": 1850
      },
      "proposal_b": {
        "id": "PROP020",
        "title": "Advanced SQL Optimization",
        "initial_rating": 950
      },
      "winner": "PROP007",
      "expected_results": {
        "PROP007": {
          "new_rating": 1851,
          "rating_change": 1
        },
        "PROP020": {
          "new_rating": 979,
          "rating_change": 29
        }
      }
    },
    {
      "description": "Lower rated proposal defeats higher rated proposal (upset)",
      "proposal_a": {
        "id": "PROP020",
        "title": "Advanced SQL Optimization", 
        "initial_rating": 950
      },
      "proposal_b": {
        "id": "PROP007",
        "title": "Machine Learning in Production",
        "initial_rating": 1850
      },
      "winner": "PROP020",
      "expected_results": {
        "PROP020": {
          "new_rating": 981,
          "rating_change": 31
        },
        "PROP007": {
          "new_rating": 1819,
          "rating_change": -31
        }
      }
    },
    {
      "description": "Proposals with similar ratings",
      "proposal_a": {
        "id": "PROP001",
        "title": "Building Scalable Microservices",
        "initial_rating": 1200
      },
      "proposal_b": {
        "id": "PROP003",
        "title": "DevOps Best Practices",
        "initial_rating": 1250
      },
      "winner": "PROP003",
      "expected_results": {
        "PROP001": {
          "new_rating": 1184,
          "rating_change": -16
        },
        "PROP003": {
          "new_rating": 1266,
          "rating_change": 16
        }
      }
    },
    {
      "description": "Draw/tie result (if supported)",
      "proposal_a": {
        "id": "PROP001",
        "title": "Building Scalable Microservices",
        "initial_rating": 1200
      },
      "proposal_b": {
        "id": "PROP002",
        "title": "Advanced React Patterns", 
        "initial_rating": 1200
      },
      "winner": "draw",
      "expected_results": {
        "PROP001": {
          "new_rating": 1200,
          "rating_change": 0
        },
        "PROP002": {
          "new_rating": 1200,
          "rating_change": 0
        }
      }
    }
  ],
  "rating_scenarios": [
    {
      "description": "Multiple comparisons creating a ranking ladder",
      "comparisons": [
        {"winner": "PROP001", "loser": "PROP002"},
        {"winner": "PROP002", "loser": "PROP003"},
        {"winner": "PROP003", "loser": "PROP004"},
        {"winner": "PROP001", "loser": "PROP003"},
        {"winner": "PROP002", "loser": "PROP004"}
      ],
      "expected_final_ratings": {
        "PROP001": 1232,
        "PROP002": 1216,
        "PROP003": 1184,
        "PROP004": 1168
      }
    }
  ],
  "edge_cases": [
    {
      "description": "Proposal with empty abstract should still calculate properly",
      "proposal_a": {
        "id": "PROP011",
        "title": "Building RESTful APIs",
        "abstract": "",
        "initial_rating": 1200
      },
      "proposal_b": {
        "id": "PROP001",
        "title": "Building Scalable Microservices",
        "initial_rating": 1200
      },
      "winner": "PROP011",
      "expected_results": {
        "PROP011": {
          "new_rating": 1216,
          "rating_change": 16
        },
        "PROP001": {
          "new_rating": 1184,
          "rating_change": -16
        }
      }
    },
    {
      "description": "Proposals with special characters in names/titles",
      "proposal_a": {
        "id": "PROP017",
        "title": "Cybersecurity Fundamentals",
        "speaker": "François Müller",
        "initial_rating": 1200
      },
      "proposal_b": {
        "id": "PROP001",
        "title": "Building Scalable Microservices",
        "initial_rating": 1200
      },
      "winner": "PROP017",
      "expected_results": {
        "PROP017": {
          "new_rating": 1216,
          "rating_change": 16
        },
        "PROP001": {
          "new_rating": 1184,
          "rating_change": -16
        }
      }
    }
