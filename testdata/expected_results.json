  "performance_benchmarks": {
    "description": "Expected performance for different dataset sizes",
    "small_dataset": {
      "proposals": 10,
      "max_comparison_time_ms": 1,
      "max_ranking_calculation_ms": 10
    },
    "medium_dataset": {
      "proposals": 50,
      "max_comparison_time_ms": 5,
      "max_ranking_calculation_ms": 100
    },
    "large_dataset": {
      "proposals": 200,
      "max_comparison_time_ms": 10,
      "max_ranking_calculation_ms": 500
    }
  },
  "validation_rules": {
    "rating_bounds": {
      "minimum": 0,
      "maximum": 3000
    },
    "k_factor_validation": {
      "standard": 32,
      "alternatives": [16, 24, 40]
    },
    "required_fields": ["id", "title", "speaker", "track"],
    "optional_fields": ["abstract", "score"]
  }
}